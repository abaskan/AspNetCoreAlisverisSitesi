@model ProductListViewModel

<div class="row">
    <div class="col-md-12">
        <div class="row">                  
            @foreach (var product in Model.Products)
            {    
                <div class="col-md-4">
                    @await Html.PartialAsync("_product", product)   
                </div>       
            }           
        </div>
    </div>
</div>